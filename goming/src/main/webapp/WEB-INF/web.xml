<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
    <display-name>Archetype Created Web Application</display-name>

    <!-- Servlet 정의 -->
    <servlet>
        <servlet-name>JoinController</servlet-name>
        <servlet-class>com.goming.user.controller.JoinController</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>LoginController</servlet-name>
        <servlet-class>com.goming.user.controller.LoginController</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>WeatherDataUpsertService</servlet-name>
        <servlet-class>com.goming.weather.controller.WeatherDataUpsertService</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>CheckUserIdService</servlet-name>
        <servlet-class>com.goming.user.controller.CheckUserIdService</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>UploadService</servlet-name>
        <servlet-class>com.goming.post.controller.UploadService</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>MyPageController2</servlet-name>
        <servlet-class>com.goming.mypage.controller.MyPageController2</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>ImgController</servlet-name>
        <servlet-class>com.goming.mypage.controller.ImgController</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>DeleteUser</servlet-name>
        <servlet-class>com.goming.mypage.controller.DeleteUser</servlet-class>
    </servlet>

    <!-- Servlet 매핑 -->
    <servlet-mapping>
        <servlet-name>JoinController</servlet-name>
        <url-pattern>/JoinController</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>LoginController</servlet-name>
        <url-pattern>/LoginController</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>WeatherDataUpsertService</servlet-name>
        <url-pattern>/WeatherDataUpsertService</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>CheckUserIdService</servlet-name>
        <url-pattern>/CheckUserIdService</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>UploadService</servlet-name>
        <url-pattern>/UploadService</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>MyPageController2</servlet-name>
        <url-pattern>/MyPageController2</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>ImgController</servlet-name>
        <url-pattern>/ImgController</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>DeleteUser</servlet-name>
        <url-pattern>/DeleteUser</url-pattern>
    </servlet-mapping>

</web-app>
