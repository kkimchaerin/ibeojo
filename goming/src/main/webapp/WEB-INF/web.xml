<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
    <display-name>Archetype Created Web Application</display-name>

    <!-- JoinController Servlet 정의 -->
    <servlet>
        <servlet-name>JoinController</servlet-name>
        <display-name>JoinController</display-name>
        <description></description>
        <servlet-class>com.goming.user.controller.JoinController</servlet-class>
    </servlet>

    <!-- LoginController Servlet 정의 -->
    <servlet>
        <servlet-name>LoginController</servlet-name>
        <display-name>LoginController</display-name>
        <description></description>
        <servlet-class>com.goming.user.controller.LoginController</servlet-class>
    </servlet>

    <!-- WeatherDataUpsertService Servlet 정의 -->
    <servlet>
        <servlet-name>WeatherDataUpsertService</servlet-name>
        <display-name>WeatherDataUpsertService</display-name>
        <description></description>
        <servlet-class>com.goming.weather.controller.WeatherDataUpsertService</servlet-class>
    </servlet>

    <!-- CheckUserIdService Servlet 정의 -->
    <servlet>
        <servlet-name>CheckUserIdService</servlet-name>
        <display-name>CheckUserIdService</display-name>
        <description></description>
        <servlet-class>com.goming.user.controller.CheckUserIdService</servlet-class>
    </servlet>

    <!-- CheckUserNickService Servlet 정의 -->
    <servlet>
        <servlet-name>CheckUserNickService</servlet-name>
        <display-name>CheckUserNickService</display-name>
        <description></description>
        <servlet-class>com.goming.user.controller.CheckUserNickService</servlet-class>
    </servlet>

    <!-- Servlet 매핑 -->
    <servlet-mapping>
        <servlet-name>JoinController</servlet-name>
        <url-pattern>/JoinController</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>LoginController</servlet-name>
        <url-pattern>/LoginController</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>WeatherDataUpsertService</servlet-name>
        <url-pattern>/WeatherDataUpsertService</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>CheckUserIdService</servlet-name>
        <url-pattern>/CheckUserIdService</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>CheckUserNickService</servlet-name>
        <url-pattern>/CheckUserNickService</url-pattern>
    </servlet-mapping>

</web-app>
